.container
  h1 SHYnet
  .row
    input#student_id(name='student_id', type='hidden')
    .col-sm-6.col-xs-12
      .panel.panel-primary
        .panel-heading
          h4 Students
        .panel-body
          .form(role='form', onkeypress="javascript:return WebForm_FireDefaultButton(event, 'FindStudent')")
            .form-group
              label.sr-only(for='SearchText') Find Student
              .input-group
                input#SearchText.form-control(name='SearchText', type='text', maxlength='20', autofocus='', placeholder="Student's first or last name")
                span.input-group-btn
                  a#FindStudent.btn.btn-warning(href="javascript:__doPostBack('FindStudent','')")
                    span.glyphicon.glyphicon-search
                    |  Search
            .form-group
              a#btnNewStudent.btn.btn-warning(data-toggle='modal', data-target='#NewStudent', href="javascript:__doPostBack('btnNewStudent','')")
                span.glyphicon.glyphicon-plus-sign
                |  New Student
          #loader(style='display:none;', role='status', aria-hidden='true')
            .text-center
              img(src='ajax-loader.gif', alt='Loading...')
              |  Searching for students...
          div
            table#gvStudents.table.table-striped.table-hover.table-condensed.table-bordered(cellspacing='0', rules='all', border='1', style='border-collapse:collapse;margin-bottom:0px;')
              tbody
                tr
                  td  No students found matching the search criteria.

    #pnlNewStudent(onkeypress="javascript:return WebForm_FireDefaultButton(event, 'NewStudentOK')")
      #NewStudent.modal
        .modal-dialog
          .modal-content
            .modal-header
              button.close(type='button', data-dismiss='modal', aria-hidden='true') &times;
              h4.modal-title New Student
            .modal-body
              .form-group
                label(for='firstName') First name
                |  
                input#firstName.form-control(name='firstName', type='text', maxlength='20', autofocus='', required='', placeholder='First name', style='margin-bottom:10px;')
              .form-group
                label(for='firstName') Last name
                |  
                input#lastName.form-control(name='lastName', type='text', maxlength='20', required='', placeholder='Last name')
            .modal-footer
              input#NewStudentCancel.btn.btn-default(type='submit', name='NewStudentCancel', value='Cancel', formnovalidate='', style='width:80px;')
              |  
              input#NewStudentOK.btn.btn-warning(type='submit', name='NewStudentOK', value='Save', style='width:80px;')
    #pnlPurchase(onkeypress="javascript:return WebForm_FireDefaultButton(event, 'PurchaseClassesOK')")
